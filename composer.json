{
    "name": "alexy-os/flight-cms",
    "description": "Root package for the flight-cms",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/alexy-os/flight-cms",
    "autoload": {
        "psr-4": {
            "FlightCms\\Core\\": "src/",
            "FlightCms\\Lib\\": "lib/",
            "FlightCms\\App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "AlexY OS"
        }
    ],
    "require": {
        "latte/latte": "3.0.x-dev",
        "flightphp/core": "^3.0",
        "tracy/tracy": "v2.10.x-dev"
    },
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "lib"
    },
    "require-dev": {
        "flightphp/tracy-extensions": "dev-master"
    }
}
